#include "stdafx.h"
#include "iostream"
#include "string"
using namespace std;
int main()
{
	setlocale(LC_ALL, "ru");
	int x,m;
	string n1[6] = { "Запад","Север","Восток","Юг","Запад","Север" };
	char c;
	cout << "Введите направление робота (N,S,E,W): ";
	cin >> c;
	switch (c)
	{
	case 'N': x = 1;
		break;
	case 'S': x = 3;
		break;
	case 'W': x = 4;
		break;
	case 'E': x = 2;
		break;
	}
	cout << "Введите команду для робота (0,1,-1): ";
	cin >> m;
	cout << n1[x - m] << endl;
	return 0;
}
